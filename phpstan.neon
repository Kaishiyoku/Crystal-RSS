includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 7 is the highest level
    level: 5

    ignoreErrors:
        - '#Unsafe usage of new static#'

    excludes_analyse:
        - ./*/*/FileToBeExcluded.php
        - ./*/*/LoginController.php
        - ./*/*/DailyArticlesChart.php

    checkMissingIterableValueType: false
