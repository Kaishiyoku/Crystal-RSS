{
    "name": "kaishiyoku/crystal-rss",
    "description": "Simple web-based RSS feed reader.",
    "keywords": ["laravel", "rss"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-pdo": "*",
        "albertcht/invisible-recaptcha": "1.9.1",
        "bensampo/laravel-enum": "1.17.1",
        "consoletvs/charts": "6.3.5",
        "davejamesmiller/laravel-breadcrumbs": "5.2.1",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "4.1.0",
        "kaishiyoku/laravel-cli-create-user": "2.2.0",
        "kaishiyoku/laravel-color-validation": "1.1.0",
        "kaishiyoku/laravel-menu": "3.0.0",
        "kaishiyoku/picofeed": "2.0.0",
        "kaishiyoku/yaml-translation": "5.8.0",
        "khill/php-duration": "1.0.6",
        "laracasts/flash": "3.0.2",
        "laravel/framework": "5.8.7",
        "laravel/horizon": "3.0.6",
        "laravel/telescope": "2.0.2",
        "laravel/tinker": "1.0.8",
        "laravelcollective/html": "5.8.0",
        "pda/pheanstalk": "4.0.0",
        "predis/predis": "1.1.1",
        "ramsey/uuid": "3.8.0",
        "spatie/flysystem-dropbox": "1.0.6",
        "spatie/laravel-backup": "6.1.1",
        "spatie/laravel-paginateroute": "2.7.0",
        "teamtnt/laravel-scout-tntsearch-driver": "7.0.0",
        "teamtnt/tntsearch": "2.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.3",
        "barryvdh/laravel-ide-helper": "2.6.1",
        "beyondcode/laravel-dump-server": "1.2.2",
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.2",
        "phpunit/phpunit": "7.5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
