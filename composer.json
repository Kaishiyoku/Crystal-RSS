{
    "name": "kaishiyoku/crystal-rss",
    "description": "Simple web-based RSS feed reader.",
    "keywords": ["laravel", "rss"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "ext-pdo": "*",
        "albertcht/invisible-recaptcha": "1.9.4",
        "bensampo/laravel-enum": "2.1.0",
        "davejamesmiller/laravel-breadcrumbs": "5.3.2",
        "doctrine/dbal": "2.10.2",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "2.0.1",
        "glorand/laravel-model-settings": "3.6.5",
        "guzzlehttp/guzzle": "6.5.5",
        "kaishiyoku/hera-rss-crawler": "0.7.0",
        "kaishiyoku/laravel-cli-create-user": "2.3.3",
        "kaishiyoku/laravel-color-validation": "1.3.0",
        "kaishiyoku/laravel-menu": "4.4.5",
        "kaishiyoku/laravel-recharts": "0.4.0",
        "khill/php-duration": "1.0.6",
        "laracasts/flash": "3.1",
        "laravel/framework": "7.25.0",
        "laravel/helpers": "1.2.0",
        "laravel/horizon": "4.3.3",
        "laravel/tinker": "2.4.2",
        "laravel/ui": "2.1.0",
        "laravelcollective/html": "6.1.2",
        "pda/pheanstalk": "4.0.2",
        "predis/predis": "1.1.2",
        "ramsey/uuid": "4.1.0",
        "sentry/sentry-laravel": "1.8.0",
        "spatie/flysystem-dropbox": "1.2.2",
        "spatie/laravel-backup": "6.11.1",
        "spatie/laravel-paginateroute": "2.7.0",
        "watson/rememberable": "4.0.0",
        "yab/laravel-scout-mysql-driver": "3.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.4.1",
        "barryvdh/laravel-ide-helper": "2.8.0",
        "facade/ignition": "2.3.6",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "4.2.0",
        "nunomaduro/larastan": "0.6.2",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "analyse": [
            ".\\vendor\\bin\\phpstan analyse"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
