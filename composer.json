{
    "name": "kaishiyoku/crystal-rss",
    "description": "Simple web-based RSS feed reader.",
    "keywords": ["laravel", "rss"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.1",
        "laravel/tinker": "1.0.2",
        "kaishiyoku/yaml-translation": "5.5.1",
        "kaishiyoku/laravel-menu": "1.4.1",
        "webup/laravel-sendinblue": "1.1.0",
        "laravelcollective/html": "5.5.1",
        "doctrine/dbal": "2.5.9",
        "laracasts/flash": "3.0.2",
        "davejamesmiller/laravel-breadcrumbs": "4.1.0",
        "albertcht/invisible-recaptcha": "1.6",
        "fguillot/picofeed": "0.1.35",
        "spatie/laravel-paginateroute": "2.5.0",
        "fideloper/proxy": "~3.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "1.7.1",
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "6.3.0",
        "barryvdh/laravel-ide-helper": "2.4.1",
        "barryvdh/laravel-debugbar": "3.0.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
